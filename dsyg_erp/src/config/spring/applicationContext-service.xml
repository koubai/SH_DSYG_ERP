	<bean id="etbDeliveryDao" class="com.cn.dsyg.dao.impl.EtbDeliveryDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>
	
	<!-- 固定资产信息 -->
	<bean id="assetsService" class="com.cn.dsyg.service.impl.EtbAssetsServiceImpl" >
		<property name="etbAssetsDao">
			<ref local="etbAssetsDao"/>
		</property>
	</bean>
	
	<bean id="etbAssetsDao" class="com.cn.dsyg.dao.impl.EtbAssetsDaoImpl">
		<property name="sqlMapClient">
