<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

	<!-- supplier -->
	<bean id="supplierService" class="com.cn.dsyg.service.impl.SupplierServiceImpl" >
		<property name="supplierDao">
			<ref local="supplierDao"/>
		</property>
	</bean>
	
	<bean id="supplierDao" class="com.cn.dsyg.dao.impl.SupplierDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>
	
	<!-- EtbProduct -->
	<bean id="etbProductService" class="com.cn.dsyg.service.impl.EtbProductServiceImpl" >
		<property name="etbProductDao">
			<ref local="etbProductDao"/>
		</property>
		<property name="dict01Dao">
			<ref local="dict01Dao"/>
		</property>
	</bean>
	
	<bean id="etbProductDao" class="com.cn.dsyg.dao.impl.EtbProductDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>
	

	<!-- PurchaseItem -->
	<bean id="etbPurchaseItemService" class="com.cn.dsyg.service.impl.EtbPurchaseItemServiceImpl" >
		<property name="etbPurchaseItemDao">
			<ref local="etbPurchaseItemDao"/>
		</property>
	</bean>
	
	<bean id="etbPurchaseItemDao" class="com.cn.dsyg.dao.impl.EtbPurchaseItemDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>

	<!-- etbPurchase -->
	<bean id="etbPurchaseService" class="com.cn.dsyg.service.impl.EtbPurchaseServiceImpl" >
		<property name="etbPurchaseDao">
			<ref local="etbPurchaseDao"/>
		</property>
		<property name="etbPurchaseItemDao">
			<ref local="etbPurchaseItemDao"/>
		</property>
	</bean>
	
	<bean id="etbPurchaseDao" class="com.cn.dsyg.dao.impl.EtbPurchaseDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>
	
	<!-- role -->
	<bean id="roleService" class="com.cn.dsyg.service.impl.RoleServiceImpl" >
		<property name="roleDao">
			<ref local="roleDao"/>
		</property>
	</bean>
	
	<bean id="roleDao" class="com.cn.dsyg.dao.impl.RoleDaoImpl">
		<property name="sqlMapClient">
			<ref bean="sqlMapClient"/>
		</property>
	</bean>

	<!-- 用户 -->
	<bean id="userService" class="com.cn.dsyg.service.impl.UserServiceImpl" >
		<property name="userDao">
			<ref local="userDao"/>
