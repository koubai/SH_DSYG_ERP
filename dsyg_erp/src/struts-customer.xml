<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="customer" extends="struts-erp" namespace="/customer">
	
		<action name="showEtbCustomerAction" class="etbCustomerAction" method="showEtbCustomerAction">
			<result name="*">/page/customer/customerlist.jsp</result>
		</action>
		
		<action name="queryEtbCustomerList" class="etbCustomerAction" method="queryEtbCustomerList">
			<result name="success">/page/customer/customerlist.jsp</result>
		</action>
		
		<action name="turnEtbCustomerPage" class="etbCustomerAction" method="turnEtbCustomerPage">
			<result name="success">/page/customer/customerlist.jsp</result>
		</action>
		
		<action name="showAddEtbCustomerAction" class="etbCustomerAction" method="showAddEtbCustomerAction">
			<result name="*">/page/customer/addcustomer.jsp</result>
		</action>
		
		<action name="addEtbCustomerAction" class="etbCustomerAction" method="addEtbCustomerAction">
			<result name="success">/page/customer/addcustomer.jsp</result>
			<result name="checkerror">/page/customer/addcustomer.jsp</result>
		</action>
		
		<action name="showUpdEtbCustomerAction" class="etbCustomerAction" method="showUpdEtbCustomerAction">
			<result name="success">/page/customer/updcustomer.jsp</result>
			<result name="checkerror">/page/customer/updcustomer.jsp</result>
		</action>
		
		<action name="updEtbCustomerAction" class="etbCustomerAction" method="updEtbCustomerAction">
			<result name="success">/page/customer/updcustomer.jsp</result>
			<result name="checkerror">/page/customer/updcustomer.jsp</result>
		</action>
		
		<action name="delEtbCustomerAction" class="etbCustomerAction" method="delEtbCustomerAction">
			<result name="success">/page/customer/customerlist.jsp</result>
			<result name="checkerror">/page/customer/customerlist.jsp</result>
		</action>
		
	</package>

</struts>