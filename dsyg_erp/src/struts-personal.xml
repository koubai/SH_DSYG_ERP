<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="personal" extends="struts-erp" namespace="/personal">
	
		<action name="showEtbPersonalAction" class="etbPersonalAction" method="showEtbPersonalAction">
			<result name="*">/page/personal/personallist.jsp</result>
		</action>
		
		<action name="queryEtbPersonalList" class="etbPersonalAction" method="queryEtbPersonalList">
			<result name="success">/page/personal/personallist.jsp</result>
		</action>
		
		<action name="turnEtbPersonalPage" class="etbPersonalAction" method="turnEtbPersonalPage">
			<result name="success">/page/personal/personallist.jsp</result>
		</action>
		
		<action name="showAddEtbPersonalAction" class="etbPersonalAction" method="showAddEtbPersonalAction">
			<result name="*">/page/personal/addpersonal.jsp</result>
		</action>
		
		<action name="addEtbPersonalAction" class="etbPersonalAction" method="addEtbPersonalAction">
			<result name="success">/page/personal/addpersonal.jsp</result>
			<result name="checkerror">/page/personal/addpersonal.jsp</result>
		</action>
		
		<action name="showUpdEtbPersonalAction" class="etbPersonalAction" method="showUpdEtbPersonalAction">
			<result name="success">/page/personal/updpersonal.jsp</result>
			<result name="checkerror">/page/personal/updpersonal.jsp</result>
		</action>
		
		<action name="updEtbPersonalAction" class="etbPersonalAction" method="updEtbPersonalAction">
			<result name="success">/page/personal/updpersonal.jsp</result>
			<result name="checkerror">/page/personal/updpersonal.jsp</result>
		</action>
		
		<action name="delEtbPersonalAction" class="etbPersonalAction" method="delEtbPersonalAction">
			<result name="success">/page/personal/personallist.jsp</result>
			<result name="checkerror">/page/personal/personallist.jsp</result>
		</action>
		
	</package>

</struts>