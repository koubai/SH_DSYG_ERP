<struts>

	<package name="user" extends="struts-erp" namespace="/user">
	
		<action name="showSelectUserAction" class="userAction" method="showSelectUserAction">
			<result name="*">/page/user/selectuser.jsp</result>
		</action>
		
		<action name="querySelectUserAction" class="userAction" method="querySelectUserAction">
			<result name="*">/page/user/selectuser.jsp</result>
		</action>
		
		<action name="turnSelectUserAction" class="userAction" method="turnSelectUserAction">
			<result name="*">/page/user/selectuser.jsp</result>
		</action>
		
	</package>

</struts>