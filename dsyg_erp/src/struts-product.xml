<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="product" extends="struts-erp" namespace="/product">
	
		<action name="delProductAction" class="productAction" method="delProductAction">
			<result name="*">/page/product/productlist.jsp</result>
		</action>
		
		<action name="showProductAction" class="productAction" method="showProductAction">
			<result name="*">/page/product/productlist.jsp</result>
		</action>
		
		<action name="queryProductAction" class="productAction" method="queryProductAction">
			<result name="*">/page/product/productlist.jsp</result>
		</action>
		
		<action name="turnProductAction" class="productAction" method="turnProductAction">
			<result name="*">/page/product/productlist.jsp</result>
		</action>
		
		<action name="showAddProductAction" class="productAction" method="showAddProductAction">
			<result name="*">/page/product/addproduct.jsp</result>
		</action>
		
		<action name="addProductAction" class="productAction" method="addProductAction">
			<result name="*">/page/product/addproduct.jsp</result>
		</action>
		
		<action name="showUpdProductAction" class="productAction" method="showUpdProductAction">
			<result name="*">/page/product/updproduct.jsp</result>
		</action>
		
		<action name="updProductAction" class="productAction" method="updProductAction">
			<result name="*">/page/product/updproduct.jsp</result>
		</action>
		
		<action name="showProductSelectPage" class="productAction" method="showProductSelectPage">
			<result name="*">/page/product/selectproduct.jsp</result>
		</action>
		
		<action name="queryProductSelectPage" class="productAction" method="queryProductSelectPage">
			<result name="*">/page/product/selectproduct.jsp</result>
		</action>
		
		<action name="turnProductSelectPage" class="productAction" method="turnProductSelectPage">
			<result name="*">/page/product/selectproduct.jsp</result>
		</action>
		
	</package>

</struts>